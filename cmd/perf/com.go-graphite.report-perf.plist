<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>com.go-graphite.report-perf</string>
        <key>ProgramArguments</key>
        <array>
            <string>/path/to/report-perf</string>
            <string>--addr=addr.of.metrics</string>
            <string>--tls</string>
            <string>--prefix=macos</string>
            <string>--interval=10</string>
        </array>
        <key>RunAtLoad</key>
        <true />
    </dict>
</plist>
